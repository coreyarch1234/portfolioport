
<div id= "signin-container">
    <h1>Login</h1>
    <%= form_tag '/login' do %>
        <div class= "form-group">
          <div><%= text_field_tag :email%> </div>
          <div><%= password_field_tag :password %></div>
         </div>
      <div>
          <%= submit_tag "Submit" %>
      </div>
</div>
    <% end %>
